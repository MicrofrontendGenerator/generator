{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": ".",
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "paths": {
            "@generator/*": ["*"]
        }
    },
    "exclude": ["node_modules"]
}